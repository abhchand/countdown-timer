{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],

  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "settings": {
    "react": {
      version: "16.3.2"
    }
  },
  "plugins": ["react", "jsx-a11y"],
  "rules": {
  }
}
